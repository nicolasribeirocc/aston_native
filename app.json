{
  "expo": {
    "name": "Aston Bank",
    "slug": "Aston Bank",
    "scheme": "astonlink",
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F28B1A"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#8ac53f",
      "androidMode": "default",
      "androidCollapsedTitle": "AstonBank"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.astonbank.com.br",
      "buildNumber": "228",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Acesse o app com sua digital",
        "NSPhotoLibraryUsageDescription": "Precisamos acessar as suas fotos para importar os seus documentos.",
        "NSCameraUsageDescription": "Precisamos acessar a sua camÃªra para importar os seus documentos.",
        "NSMicrophoneUsageDescription": "Precisamos acessar seu microfone para gravar videos.",
        "NSContactsUsageDescription": "Precisamos acessar os seus contatos para localizar contas."
      }
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "astonlink"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "package": "app.astonbank.com.br",
      "versionCode": 132,
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA",
        "INTERNET",
        "SYSTEM_ALERT_WINDOWS",
        "READ_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
    }
  }
}
